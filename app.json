{
  "expo": {
    "name": "Exptra",
    "slug": "exptra-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "exptraapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.exptra.app",
      "infoPlist": {
        "NSUserNotificationsUsageDescription": "Allow notifications to stay up to date with the latest news and updates."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2196F3",
        "foregroundImage": "./assets/images/android-chrome-512x512.png",
        "backgroundImage": "./assets/images/android-chrome-512x512.png",
        "monochromeImage": "./assets/images/android-chrome-512x512.png"
      },
      "package": "com.exptra.app",
      "permissions": ["android.permission.POST_NOTIFICATIONS"],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon-32x32.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2196F3",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e7a1b279-4d03-4363-af65-cbdbe81790f3"
      }
    }
  }
}
